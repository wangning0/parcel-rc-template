{
  "name": "parcel-rc-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel ./example/index.html",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "build": "npm-run-all clean:* --parallel build:*",
    "build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel src/ --out-dir lib/",
    "build:esm": "babel src/ --out-dir esm/",
    "clean:commonjs": "rimraf lib",
    "clean:umd": "rimraf dist",
    "clean:esm": "rimraf esm",
    "prestart": "npm run prebuild && cp ./.babelrc-template ./.babelrc && rimraf .cache",
    "prebuild": "rimraf ./.babelrc",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "save-dev": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/plugin-external-helpers": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-es2015": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-stage-0": "^7.0.0-beta.38",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.6.1",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.5.1",
    "prettier": "^1.10.2",
    "react-router": "^3.2.0",
    "rollup": "^0.55.1",
    "style-loader": "^0.19.1"
  },
  "repository": "https://github.com/wangning0/parcel-rc-template",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
